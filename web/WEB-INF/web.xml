<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ingresousuario</servlet-name>
        <servlet-class>controlador.ingresousuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registro</servlet-name>
        <servlet-class>modelo.registro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registro2</servlet-name>
        <servlet-class>modelo.registro2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>controlador.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mlogin</servlet-name>
        <servlet-class>modelo.mlogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ingresousuario</servlet-name>
        <url-pattern>/ingresousuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registro</servlet-name>
        <url-pattern>/registro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registro2</servlet-name>
        <url-pattern>/registro2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mlogin</servlet-name>
        <url-pattern>/mlogin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
